{
    "AWSEBDockerrunVersion": 2,
    "volumes": [],
    "containerDefinitions": [{
            "name": "pact",
            "image": "dius/pact_broker",
            "essential": true,
            "memory": 256,
            "portMappings": [{
                "hostPort": 80,
                "containerPort": 80
            }],
            "environment": [{
                    "name": "PACT_BROKER_DATABASE_NAME",
                    "value": "${RDS_DB_NAME}"
                },
                {
                    "name": "PACT_BROKER_DATABASE_PASSWORD",
                    "value": "${RDS_PASSWORD}"
                },
                {
                    "name": "PACT_BROKER_DATABASE_HOST",
                    "value": "${RDS_HOSTNAME}"
                },
                {
                    "name": "PACT_BROKER_DATABASE_USERNAME",
                    "value": "${RDS_USERNAME}"
                }
            ],
            "mountPoints": []
        }
    ]
}
